#PBS -V
#PBS -l mppwidth=1
#PBS -l mppnppn=1
#PBS -l mppdepth=1
#PBS -l walltime=0:10:00
#PBS -o serial.stdout
#PBS -j oe

cd $PBS_O_WORKDIR
aprun -d 1 -n 1 -N 1 ./serial -n 500
aprun -d 1 -n 1 -N 1 ./serial -n 1000
aprun -d 1 -n 1 -N 1 ./serial -n 10000
aprun -d 1 -n 1 -N 1 ./serial -n 20000
aprun -d 1 -n 1 -N 1 ./serial -n 40000

aprun -d 1 -n 1 -N 1 ./serial -n 10000 -o serial10000.txt
