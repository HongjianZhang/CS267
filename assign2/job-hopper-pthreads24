#PBS -V
#PBS -l mppwidth=1
#PBS -l mppnppn=1
#PBS -l mppdepth=24
#PBS -l walltime=0:10:00
#PBS -o pthreads24.stdout
#PBS -j oe

cd $PBS_O_WORKDIR
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 500 
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 1000 
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 10000 
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 40000 
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 100000
 
aprun -d 24 -n 1 -N 1 ./pthreads -p 24 -n 10000 -o pthreads10000.txt 
