#PBS -V
#PBS -l mppwidth=1
#PBS -l mppnppn=1
#PBS -l mppdepth=24
#PBS -S /bin/bash
#PBS -l walltime=0:10:00
#PBS -o openmp24.stdout
#PBS -j oe

cd $PBS_O_WORKDIR

export OMP_NUM_THREADS=12
aprun -d 12 -n 1 -N 1 ./openmp -n 500
aprun -d 12 -n 1 -N 1 ./openmp -n 1000
aprun -d 12 -n 1 -N 1 ./openmp -n 5000
aprun -d 12 -n 1 -N 1 ./openmp -n 10000
aprun -d 12 -n 1 -N 1 ./openmp -n 20000
aprun -d 12 -n 1 -N 1 ./openmp -n 40000

aprun -d 12 -n 1 -N 1 ./openmp -n 10000 -o openmp10000.txt
