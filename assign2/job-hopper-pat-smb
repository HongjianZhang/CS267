cd $PBS_O_WORKDIR
export PAT_RT_HWPC=1
aprun -d 1 -n 1 -N 1 ./serial_mb+pat -n 500
aprun -d 1 -n 1 -N 1 ./serial_mb+pat -n 1000
aprun -d 1 -n 1 -N 1 ./serial_mb+pat -n 10000
aprun -d 1 -n 1 -N 1 ./serial_mb+pat -n 20000
aprun -d 1 -n 1 -N 1 ./serial_mb+pat -n 40000
